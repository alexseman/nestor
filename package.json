{
    "name": "nestor",
    "version": "0.0.1",
    "description": "",
    "type": "module",
    "scripts": {
        "clean": "./node_modules/.bin/rimraf dist",
        "dev": "NODE_ENV=development nodemon",
        "build": "tsc",
        "db:migrations:up": "./node_modules/db-migrate/bin/db-migrate up --migrations-dir ./dist/database/migrations",
        "db:migrations:down": "./node_modules/db-migrate/bin/db-migrate down --migrations-dir ./dist/database/migrations",
        "db:migrations:reset": "./node_modules/db-migrate/bin/db-migrate reset --migrations-dir ./dist/database/migrations",
        "db:seed": "node ./dist/database/seeders/groupsTable.seed.js && node ./dist/database/seeders/personsTable.seed.js",
        "lint": "./node_modules/.bin/eslint --ext .js,.ts",
        "prettier": "./node_modules/.bin/prettier --write ."
    },
    "keywords": [],
    "author": "Alex Seman <mail@alexseman.com>",
    "license": "UNLICENSED",
